    </assemblyBinding>
  </runtime>
  <connectionStrings>
    <add name="LoggingConnectionString"
      connectionString="Data Source=localhost;Initial Catalog=imdbgraph;Integrated Security=True;"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>